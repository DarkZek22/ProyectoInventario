@model InventarioUX.Models.MOV_ENTRADA
@{
    ViewBag.Title = "CheckOut";
}

<h2>Seleccione un proveedor</h2>

<fieldset>
    <legend>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true, "", new { @class = "text-danger"})
            <table class="table">
                <tr>
                    <td>@Html.LabelFor(model => model.PROVEEDORID, "PROVEEDORID", htmlAttributes: new { @class = "control-label col-md-2"})</td>
                    <td>@Html.DropDownList("PROVEEDORID", null, htmlAttributes: new { @class = "form-control" })</td>
                    @Html.ValidationMessageFor(model => model.PROVEEDORID, "", new { @class = "text-danger" })
                </tr>
                <tr>
                    <td>     </td>
                    <td><input type="submit" value="Ingresar"/></td>
                </tr>
            </table>
        }
    </legend>
</fieldset>
