@model InventarioUX.Models.ASIGNACIONES
@{
    ViewBag.Title = "CheckOut";
}

<h2>Seleccione un departamento</h2>

<fieldset>
    <legend>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <table class="table">
                <tr>
                    <td>@Html.LabelFor(model => model.DEPARTAMENTOID, "DEPARTAMENTOID", htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>@Html.DropDownList("DEPARTAMENTOID", null, htmlAttributes: new { @class = "form-control" })</td>
                    @Html.ValidationMessageFor(model => model.DEPARTAMENTOID, "", new { @class = "text-danger" })
                </tr>
                <tr>
                    <td>     </td>
                    <td><input type="submit" value="Ingresar" /></td>
                </tr>
            </table>
        }
    </legend>
</fieldset>